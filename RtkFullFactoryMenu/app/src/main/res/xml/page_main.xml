<?xml version="1.0" encoding="UTF-8"?>
<PreferenceContainer logic="com.realtek.tvfactory.MainLogic">

    <Preference id="@+id/page_sw_info" title="@string/str_sw_info" />

    <Preference id="@+id/factory_select" title="@string/str_select_title"/>

    <Preference id="@+id/system_info" title="@string/str_system_info"/>

    <Preference id="@+id/page_picture" title="@string/str_picture" />

    <if condition="true" product="rtd6748" >
        <Preference id="@+id/page_oled" title="@string/str_OLED"/>
    </if>

    <Preference id="@+id/page_sound" title="@string/str_sound" />

    <Preference id="@+id/page_tune" title="@string/str_tuning_setting" />

    <Preference id="@+id/page_design_mode" title="@string/str_design_mode" />

    <Preference id="@+id/page_user" title="@string/str_user" visible="false"/>

    <Preference id="@+id/page_others" title="@string/str_others" visible="false"/>

    <Preference id="@+id/page_ssc" title="@string/str_ssc_title" />

    <Preference id="@+id/page_ch_default" title="@string/str_ch_default"/>

    <Preference id="@+id/reset" title="@string/str_reset" />

    <Preference id="@+id/android_reset" title="@string/str_Android_reset"/>

    <StatePreference id="@+id/CustomerPowerOnMode" title="@string/str_CustomerPowerOnMode" cycleEnabled="true" visible="false">
        <entry name="@string/str_on" enabled="true" />
        <entry name="@string/str_off" enabled="true" />
        <entry name="@string/power_memory" enabled="true" />
    </StatePreference>
</PreferenceContainer>