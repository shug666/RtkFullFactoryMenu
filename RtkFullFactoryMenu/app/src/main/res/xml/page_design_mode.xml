        <entry name="@string/str_on" enabled="true" />
    </StatePreference>

    <Preference id="@+id/wb_adjust" title="@string/str_wb_adjust" />

    <StatePreference id="@+id/wb_switch" title="@string/str_wb_switch" cycleEnabled="true">
        <entry name="@string/str_off" enabled="true" />
        <entry name="@string/str_on" enabled="true" />
    </StatePreference>

    <StatePreference id="@+id/uart_logcat" title="@string/str_uart" cycleEnabled="true">
        <entry name="@string/str_on" enabled="true" />
        <entry name="@string/str_off" enabled="true" />
    </StatePreference>

    <Preference id="@+id/white_pattern" title="@string/str_white_pattern" />

    <Preference id="@+id/cancel_pattern" title="@string/str_cancel_pattern" />

    <Preference id="@+id/clear_app" title="@string/str_clear_app" />

