<PreferenceContainer logic="com.realtek.tvfactory.designMode.DesignModeLogic">

<!--    <Preference id="@+id/page_offrs" title="@string/str_OffRS_Mode" />-->

    <StatePreference id="@+id/item_power_mode" title="@string/str_power_mode" cycleEnabled="true">
        <entry name="@string/power_direct" enabled="true" />
        <entry name="@string/power_secondary" enabled="true" />
        <entry name="@string/power_memory" enabled="true" />
    </StatePreference>

    <StatePreference id="@+id/test_pattern" title="@string/str_test_pattern" index="0" cycleEnabled="true" >
        <entry name="@string/str_off" index="0" enabled="true" >
            <boolean name="off" value="true" />
        </entry>
        <entry name="@string/str_white" index="1"  enabled="true" />
        <entry name="@string/str_red" index="2"  enabled="true" />
        <entry name="@string/str_green" index="3"  enabled="true" />
        <entry name="@string/str_blue" index="4"  enabled="true" />
        <entry name="@string/str_black" index="5"  enabled="true" />
    </StatePreference>

    <Preference id="@+id/aging_mode" title="@string/str_AgingMode" />

    <StatePreference id="@+id/factory_test" title="@string/str_factory_test" cycleEnabled="true">
        <entry name="@string/str_off" enabled="true" />
        <entry name="@string/str_on" enabled="true" />
    </StatePreference>

    <StatePreference id="@+id/display_logo" title="@string/str_display_logo" cycleEnabled="true">
        <entry name="@string/str_on" enabled="true" />
        <entry name="@string/str_off" enabled="true" />
    </StatePreference>

    <StatePreference id="@+id/BOE_cmd_enable" title="@string/str_uart_cmd_enable" cycleEnabled="true">
        <entry name="@string/str_off" enabled="true" />
        <entry name="@string/str_on" enabled="true" />
    </StatePreference>

    <Preference id="@+id/m_mode" title="@string/str_manage_mode" />

    <Preference id="@+id/factory_remote" title="@string/str_factory_remote" />

    <StatePreference id="@+id/key_upgrade_force" title="@string/str_key_upgrade_force" cycleEnabled="true">
